<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="FinanceEntities"
                    namespace="FinanceEntities">
  <class name="Income">
    <id name="Id" type="guid">
      <generator class="guid.comb" />
    </id>
    <property name="Date" />
    <property name="Description" />
    <property name="Amount" />
    <property name="Notes" />
    <property name="BankCleared" />
    <property name="PayingInSlip" />
    <property name="GiftAidStatus" type="Repositories.PaymentGiftAidStatusType" />

    <many-to-one name="PaymentType" class="PaymentType" column="PaymentTypeId" />
    <many-to-one name="BudgetType" class="BudgetType" column="BudgetTypeId" />
    <many-to-one name="FundType" class="FundType" column="FundTypeId" />
  </class>
</hibernate-mapping>