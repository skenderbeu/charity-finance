<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="FinanceEntities"
                    namespace="FinanceEntities">
  <class name="Income">
    <id name="Id" type="guid">
      <generator class="guid.comb" />
    </id>
    <property name="Date" />
    <property name="Description" />
    <property name="PaymentType" type="PaymentType" />
    <property name="Amount" />
    <property name="BudgetType" type="BudgetType" />
    <property name="Notes" />
    <property name="FundType" type="FundType" />
    <property name="BankCleared" />
    <property name="PayingInSlip" />
    <property name="GiftAidStatus" type="Repositories.PaymentGiftAidStatusType" />
  </class>
</hibernate-mapping>